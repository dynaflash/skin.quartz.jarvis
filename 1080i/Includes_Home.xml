    <include name="MiniPlayer_Home">
        <control type="group" description="Now Playing Home MinPlayer">
            <visible>[Player.HasVideo | Player.HasAudio] + [!Window.IsActive(DialogBusy.xml) | !Window.IsActive(VideoFullscreen.xml)]</visible>
            <animation effect="fade" tween="cubic" easing="inout" time="1200">VisibleChange</animation>
            <posx>100</posx>
            <posy>15</posy>
            <width>800</width>
            <height>100</height>
            <control type="image" description="MiniPlayer Background Image">
                <!--<visible>!ControlGroup(599).HasFocus(id)</visible>-->
                <posx>-15</posx>
                <posy>-20</posy>
                <width>755</width>
                <height>125</height>
                <aspectratio>stretch</aspectratio>
                <texture>img/HomeMenuBar.png</texture>
            </control>
            <control type="image" description="MiniPlayer Background Image Focused">
                <visible>ControlGroup(599).HasFocus(id)</visible>
                <posx>-15</posx>
                <posy>-6</posy>
                <width>755</width>
                <height>84</height>
                <aspectratio>stretch</aspectratio>
                <texture border="50,0,50,0">home/HomeBar.png</texture>
            </control>
            <ondown>700</ondown>
            <control type="group" description="Playing Thumb">
                <posx>0</posx>
                <posy>0</posy>
                <width>75</width>
                <height>75</height>
                <!-- Video Cover -->
                <control type="image">
                    <visible>Player.HasVideo</visible>
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>75</width>
                    <height>75</height>
                    <texture fallback="OSD/VideoSettings.png">$INFO[VideoPlayer.Cover]</texture>
                    <aspectratio>keep</aspectratio>
                </control>
                <!-- Music Cover -->
                <control type="image">
                    <visible>Player.HasAudio</visible>
                    <posx>0</posx>
                    <posy>0</posy>
                    <width>75</width>
                    <height>75</height>
                    <texture fallback="HUD/Volume.png">$INFO[MusicPlayer.Cover]</texture>
                    <aspectratio>keep</aspectratio>
                </control>
            </control>
            <control type="group" description="Playing Info Label">
                <control type="label">
                    <posx>100</posx>
                    <posy>0</posy>
                    <width>560</width>
                    <height>40</height>
                    <align>left</align>
                    <aligny>top</aligny>
                    <font>size16</font>
                    <textcolor>Grey3</textcolor>
                    <shadowcolor>ShadowDark</shadowcolor>
                    <scrollspeed>30</scrollspeed>
                    <pauseatend>8000</pauseatend>
                    <scroll>true</scroll>
                    <label>$INFO[VideoPlayer.ChannelNumberLabel,,[COLOR=HL] • [/COLOR]] $INFO[VideoPlayer.ChannelName,,[COLOR=HL] • [/COLOR]] $INFO[VideoPlayer.TVShowTitle,,[COLOR=HL] • [/COLOR]Season] $INFO[VideoPlayer.Season,, Episode] $INFO[VideoPlayer.Episode,,[COLOR=HL] • [/COLOR]] $INFO[MusicPlayer.Artist,,[COLOR=HL] • [/COLOR]] $INFO[MusicPlayer.Album,,[COLOR=HL] • [/COLOR]] $INFO[Player.Title,, ] </label>
                </control>
            </control>
            <!-- Mini Player Controls right under the player info -->
            <control type="group" description="Mini Player Controls" id="599">
                <posx>100</posx>
                <posy>35</posy>
                <width>800</width>
                <height>50</height>
                <align>left</align>
                <onhidden>9000</onhidden>
                <!-- Detect if there is focusable content in shelf and go there on down -->
                <ondown condition="Control.IsVisible(311)|Control.IsVisible(312)|Control.IsVisible(313)|Control.IsVisible(314)|Control.IsVisible(315)">700</ondown>
                <!-- Else go back to the main nav bar on down -->
                <ondown>9000</ondown>
                <defaultcontrol>602</defaultcontrol>

                <!-- Start Main Video Playback Controls -->
                <control type="button" id="600">
                    <posx>0</posx>
                    <width>50</width>
                    <height>35</height>
                    <texturenofocus colordiffuse="Grey3">OSD/SkipBack.png</texturenofocus>
                    <texturefocus colordiffuse="HL">OSD/SkipBack.png</texturefocus>
                    <onup>606</onup>
                    <onleft>599</onleft>
                    <onright>601</onright>
                    <ondown condition="Control.IsVisible(311)|Control.IsVisible(312)|Control.IsVisible(313)|Control.IsVisible(314)|Control.IsVisible(315)">700</ondown>
                    <!-- Else go back to the main nav bar on down -->
                    <ondown>9000</ondown>
                    <onclick>PlayerControl(Previous)</onclick>
                </control>
                <control type="button" id="601">
                    <posx>50</posx>
                    <width>50</width>
                    <height>35</height>
                    <texturenofocus colordiffuse="Grey3">OSD/Rewind.png</texturenofocus>
                    <texturefocus colordiffuse="HL">OSD/Rewind.png</texturefocus>
                    <onleft>600</onleft>
                    <onright>602</onright>
                    <onup>606</onup>
                    <ondown condition="Control.IsVisible(311)|Control.IsVisible(312)|Control.IsVisible(313)|Control.IsVisible(314)|Control.IsVisible(315)">700</ondown>
                    <!-- Else go back to the main nav bar on down -->
                    <ondown>9000</ondown>
                    <onclick>PlayerControl(Rewind)</onclick>
                </control>
                <control type="togglebutton" id="602">
                    <posx>90</posx>
                    <width>50</width>
                    <height>35</height>
                    <texturenofocus colordiffuse="Grey3">OSD/Play.png</texturenofocus>
                    <texturefocus colordiffuse="HL">OSD/Play.png</texturefocus>
                    <usealttexture>Player.Playing</usealttexture>
                    <alttexturenofocus colordiffuse="Grey3">OSD/Pause.png</alttexturenofocus>
                    <alttexturefocus colordiffuse="HL">OSD/Pause.png</alttexturefocus>
                    <onleft>601</onleft>
                    <onright>603</onright>
                    <!-- Detect if there is focusable content in shelf and go there on down -->
                    <ondown condition="Control.IsVisible(311)|Control.IsVisible(312)|Control.IsVisible(313)|Control.IsVisible(314)|Control.IsVisible(315)">700</ondown>
                    <!-- Else go back to the main nav bar on down -->
                    <ondown>9000</ondown>
                    <onclick>PlayerControl(Play)</onclick>
                </control>
                <control type="button" id="603">
                    <posx>120</posx>
                    <width>50</width>
                    <height>35</height>
                    <texturenofocus colordiffuse="Grey3">OSD/Stop.png</texturenofocus>
                    <texturefocus colordiffuse="HL">OSD/Stop.png</texturefocus>
                    <onup>606</onup>
                    <onleft>602</onleft>
                    <onright>604</onright>
                    <!-- Detect if there is focusable content in shelf and go there on down -->
                    <ondown condition="Control.IsVisible(311)|Control.IsVisible(312)|Control.IsVisible(313)|Control.IsVisible(314)|Control.IsVisible(315)">700</ondown>
                    <!-- Else go back to the main nav bar on down -->
                    <ondown>9000</ondown>

